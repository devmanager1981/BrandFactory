# Bria API Configuration (Production - Cloud API)
# Use your Production API key from https://bria.ai/
BRIA_API_TOKEN=your_production_api_token_here
BRIA_API_BASE_URL=https://engine.prod.bria-api.com/v1

# Optional: Hugging Face Token (for private models)
HF_TOKEN=your_huggingface_token_here

# Generation Settings
DEFAULT_SEED=42
DEFAULT_INFERENCE_STEPS=50
DEFAULT_GUIDANCE_SCALE=7.5

# Output Settings
OUTPUT_DIR=output
ENABLE_C2PA=true
